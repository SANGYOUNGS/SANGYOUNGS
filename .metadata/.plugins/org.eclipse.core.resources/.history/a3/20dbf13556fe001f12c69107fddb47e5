package com.exam;

import java.util.*;
public class Ex002 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int a, b, c ;
		a = sc.nextInt();
		b = sc.nextInt();
		c = sc.nextInt();
		if(a > b && b > c) {
			System.out.println(a);
		}
	}

}
