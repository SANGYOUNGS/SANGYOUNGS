package com.forEx;

							 // j = 1 j < 10 
/* 	 별이 빛나는에 
 *	**********
 *	**********
 *	********** 		
 *	********** 
 * **********
 * 
 *  for( ){
 *  for(){}}
 * 
 */




public class ForExam01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//System.out.println("**********");
		int i;
		for (int j = 2; j<=4; j++) {
		 for (i =1 ; i <= 3; i ++ ) {
			System.out.printf("%d x %d = %d\f", j,i, j*i);
		 }
		 	System.out.println();
		}
	}

}
