package com.ex;
/* ASCII 입력 받기
 * 
 *  기본으로 알아야할 아스키 코드 값
 * 
 * 		48 ~ 57 -> 0 ~ 9
 * 		65 ~ 90 -> A ~ Z 
 * 		97 ~ 122 -> a ~ z   
 * 
 *   '\r' => 13
 * 	  '\n' => 10
 *	  '\t' => 9
 *	  '/b' => 8	 	
 */


import java.io.*;
public class InputEx01 {
	
	public static void main(String[] args) {
		System.out.println("문자입력 : ");
		int i = System.in.read();
		System.out.println("ASCII 값 : " +  i);
		
	}
}
