package com.forEx;

public class WhileEx01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int i = 2 , j=1; // 초기값
		
		while(i <=9) {
			j=1;
			while(j <=9) {
				System.out.printf("%d x %d = %d%n",i,j,i*j);
				j++;
			}
			System.out.println();
			i++;
		}
	}

}
