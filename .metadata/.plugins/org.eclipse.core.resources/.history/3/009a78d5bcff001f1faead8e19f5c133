package com.exam;

import java.util.*;

public class MathMaxEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int a, b, c ;
		a = sc.nextInt();
		b = sc.nextInt();
		c = sc.nextInt();
		if ( a < b) {
			int temp= a; 
			a = b;
			b = temp;		
		}
		if (a < c) {
			int temp = a;
			a = c;
			c = temp;
		}
		if (b < c) {
			int temp = b;
			b = c;
			c = temp;
		}
		System.out.println(a + ">" + b + ">" + c); 
	}
}
