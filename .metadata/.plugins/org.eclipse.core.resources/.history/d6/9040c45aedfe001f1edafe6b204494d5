package com.ifex;
import java.util.*;
/*
 *  주민번호 
 *  1800 0 남자 9 여자
 * 	 1900 1남자  2 여자
 * 	 2000 3남자  4여자
 * 
 */
public class ifEx08 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc =new Scanner(System.in);
		System.out.println("주민번호 입력");
		char a;
		a = sc.next().charAt(7);
		System.out.println(a);
	}

}
