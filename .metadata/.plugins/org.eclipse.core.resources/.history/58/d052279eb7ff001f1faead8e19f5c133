package com.exam;

import java.util.*;

public class MathMaxEx {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int a,b,c;
		a = sc.nextInt();
		b = sc.nextInt();
		c = sc.nextInt();
		int max = Math.max(a,Math.max(b, c));
		System.out.print(max);
	}
}
