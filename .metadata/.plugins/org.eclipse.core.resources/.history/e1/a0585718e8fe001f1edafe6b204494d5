package com.ifex;
import java.util.*;
public class ifEx07 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		System.out.println("알파벳을 입력해주세요");
		char a =sc.next().charAt(0);
		if(a < 65){
			System.out.println("대문자 입니다.");
		} 
	}

}
